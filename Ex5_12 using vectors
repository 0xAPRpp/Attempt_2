# include <iostream>
# include <cctype>
# include <iomanip>
# include <array>
# include <cmath>
# include <vector>

using namespace std;

int main()
{
	size_t count{};

	std::cin >> count;

	std::vector<size_t> vector_of_heights(count);
	size_t total{};

	for (size_t i{}, value{}; i < count;)
	{
		std::cin >> value;
		if (value != 0)
		{
			vector_of_heights[i] = value;
			total += vector_of_heights[i];
			i++;
		}
		else
			cout << "0 is not allowed" << endl;
	}

	cout << total / count << endl;



	system("pause");
}	
