# include <iostream>

using namespace std;

int main()
{
	double value{};

	cin >> value;
	
	for (int i{ -3 }; i <= 3; i++)
	{
		cout << pow(value, i) << endl;
	}
	system("pause");
}

double pow(double base, int exp)
{
	double result{1};
	if (exp < 0)
		exp *= -1;
	else if (exp == 0)
		return 1.0;
	for (size_t i{}; i < exp; i++)
	{
		result *= base;
	}

	if (exp > 0)
		return result;
	if (exp < 0)
		return 1.0 / result;
}
