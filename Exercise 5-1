/*Exercise 5-1. Write a program that outputs the squares of the odd integers from 1 up to
a limit that is entered by the user.*/

# include <iostream>
# include <cctype>
# include <iomanip>
# include <array>
# include <cmath>
# include <vector>

using namespace std;

int main()
{
	size_t limit{};
	std::cin >> limit;

	for (size_t i{ 1 }; i <= limit; i += 2)
	{
		std::cout << i * i << std::endl;
	}

	system("pause");
}	
