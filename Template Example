# include <iostream>
# include <string>

using namespace std;
template <typename T> T ud_t_larger(T value1, T value2);

int main(int argc, char* argv[])
{
	cout << ud_t_larger(1, 2) << endl;
	cout << ud_t_larger(2.1, 3.1) << endl;
	cout << ud_t_larger('a', 'b') << endl;
	cout << ud_t_larger("template", "function") << endl;
	cout << ud_t_larger(4.1f, 0.2f) << endl;
	cout << ud_t_larger(3ll, 4ll) << endl;
		
		
	system("pause");
}

template <typename T> T ud_t_larger(T value1, T value2)
{
	return value1 > value2 ? value1 : value2;
}
