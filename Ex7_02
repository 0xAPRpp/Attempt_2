# include <iostream>
# include <string>
using namespace std;

int main()
{
	std::string text{};
	std::getline(cin, text, '\n');

	size_t vowels{}, consonants{};

	for (size_t i{}; i < std::size(text); i++)
	{
		switch (text[i])
		{
		case 'a': case 'A': case 'e': case 'E': case 'i': case 'I':  case 'o': case 'O':
		case 'u': case 'U':
			vowels++;
			break;
		default:
			if(tolower(isalpha(text[i])))
				consonants++;
			break;
		}
	}

	cout << vowels << " " << consonants << endl;

	system("pause");
}
