# include <iostream>
# include <string>

using namespace std;
size_t ud_fibonacci(size_t n);

int main(int argc, char* argv[])
{
	cout << ud_fibonacci(stoi(argv[1])) << endl;
		
	system("pause");
}

size_t ud_fibonacci(size_t n)
{
	if (n <= 2)
		return 1;

	return ud_fibonacci(n-2) + ud_fibonacci(n - 1);
}
